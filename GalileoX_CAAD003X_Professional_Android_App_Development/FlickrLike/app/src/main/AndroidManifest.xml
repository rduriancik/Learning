<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.robert.flickrlike">

    <application
        android:name=".FlickrLikeApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".mainActivity.ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".likedPhotosActivity.LikedPhotosActivity"
            android:parentActivityName=".mainActivity.ui.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".mainActivity.ui.MainActivity" />
        </activity>
        <activity
            android:name=".photoActivity.ui.PhotoActivity"
            android:parentActivityName=".mainActivity.ui.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".mainActivity.ui.MainActivity" />
        </activity>
    </application>

</manifest>